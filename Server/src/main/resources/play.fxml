      <ListView fx:id="serverList" layoutX="21.0" layoutY="14.0" prefHeight="373.0" prefWidth="291.0" />
      <Text fx:id="numClients" layoutX="385.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Clients connected: " wrappingWidth="164.125" />
      <VBox layoutX="385.0" layoutY="101.0" prefHeight="200.0" prefWidth="172.0" spacing="20.0">
         <children>
            <Button fx:id="launchAgainBtn" mnemonicParsing="false" onAction="#launchAgainHandle" text="Click to Launch Server" />
